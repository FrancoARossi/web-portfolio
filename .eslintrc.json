{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "next",
    "airbnb-base",
    "prettier"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "@typescript-eslint",
    "prettier"
  ],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "semi": true
      }
    ],
    "import/extensions": ["off"],
    "no-console": ["warn"]
  }
}
